@model Server_v0._0.Models.Order
@{
    ViewBag.Title = "Добавить заказ";
}
<form asp-action="create" asp-controller="OrdersController">
    <div class="form-group">
        <label asp-for="Price" class="control-label">Цена заказа</label>
        <input type="number" asp-for="Price" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>