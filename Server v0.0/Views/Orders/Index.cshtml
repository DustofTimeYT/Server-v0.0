@model IEnumerable<Server_v0._0.Models.Order>
@{
    ViewBag.Title = "Все пользователи";
}
<a asp-action="Create">Добавить заказ</a>
<table class="table">
     
    @foreach (var item in Model)
    {
        <tr><td>@item.Id</td>
            <td>
                <a asp-action="Details" asp-controller="Oders" asp-route-id="@item.Id">Подробнее</a> |
                <a asp-action="Edit" asp-controller="Oders" asp-route-id="@item.Id">Изменить</a> |
                <a asp-action="Delete" asp-controller="Oders" asp-route-id="@item.Id">Удалить</a>
            </td>
        </tr>
    }
</table>